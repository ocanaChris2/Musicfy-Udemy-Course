{
  "name": "spotifyclone",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "author": "Agustin Navarro Galdon",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
